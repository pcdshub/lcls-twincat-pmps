    {attribute 'pytmc' := '
        pv: EEnergy
        io: i
        field: DESC Electron Energy
        field: EGU GeV
    '}
    fElectronEnergyVal : LREAL;

    {attribute 'pytmc' := '
        pv: EEnergyValid
        io: i
        field: DESC Electron Energy Valid
    '}
