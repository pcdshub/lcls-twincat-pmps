    {attribute 'pytmc' := '
        pv: UND
        link: USEG:UNDH:
    '}
    fbHgvpu : FB_Hgvpu;

END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[// Update PVs
